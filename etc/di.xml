                <item name="MichalPromo" xsi:type="object">SwiftOtter\Spaghetti\SpecialAction\MichalPromo</item>
                <item name="TenthCustomer" xsi:type="object">SwiftOtter\Spaghetti\SpecialAction\TenthCustomer</item>
                <item name="FirstCustomersPromo" xsi:type="object">SwiftOtter\Spaghetti\SpecialAction\FirstCustomersPromo</item>
            </argument>
        </arguments>
    </type>
</config>
